
{
  "manifest_version": 2,

  "name": "Moji Extension",
  "description": "This extension will help you organize your content from web.",
  "version": "0.2",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ]
}
